@Article{         clauset:2007,
  author        = {Clauset, A. and Shalizi, C.R. and Newman, MEJ},
  title         = {{Power-law distributions in empirical data}},
  journal       = {Arxiv preprint arXiv:0706.1062},
  year          = {2007},
  annote        = {Els autors alerten contra la tendència en la literatura
                  d'anàlisi de xarxes a identificar distribucions potencials
                  del grau sense anàlisis estadístiques rigoroses. En
                  aquest \emph{paper} fan una revisió de diferents
                  distribucions potencials del grau que han estat publicades
                  en la literatura científica i demostren que algunes
                  d'elles no són potencials sinó que són altres tipus de
                  distribucions que també es caracteritzen per ser
                  asimètriques i tenir una cua (\emph{tail}) dreta rellevant
                  (pe exponencial, lognormal,...). Els autors proposen un
                  mètode estadístic per fer estimacions rigoroses i
                  proporcionen programari (escrit en \texttt{R} i en
                  \texttt{C}) per implementar el seu mètode. He sotmès les
                  dades de la xarxa de col·laboració de Debian a aquest
                  mètode però com que no domino els procediments
                  estadístics que proposen he preferit deixar per a la tesi
                  el tractament matemàtic rigorós de les distribucions de
                  paquets per persona i de nombre de col·laboradors per
                  persona i només conjecturar aquí el caràcter potencial
                  amb caiguda exponencial i punt de tall definit. [URL
                  consultat setembre 2008]},
  url           = {http://arxiv.org/abs/0706.1062}
}

