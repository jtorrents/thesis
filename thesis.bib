@Article{         merton:1942,
  author        = {Robert K. Merton},
  title         = {Science and technology in a democratic order},
  journal       = {Journal of Legal and Political Sociology},
  year          = {1942},
  volume        = {1},
  pages         = {115--126},
  annote        = {Un dels articles més importants de la sociologia de la
                  ciència del segle XX. Merton planteja que la paraula
                  Ciència és enganyosament àmplia, es pot referir a tres
                  àmbits diferents però relacionats entre sí: (a) Ciència
                  com a conjunt de mètodes per mitjà dels quals es
                  certifica el coneixement, (b) Ciència com a conjunt de
                  coneixments demostrats a través d'aquests mètodes i (c)
                  Ciència com a conjunt de valors i normes culturals que
                  governen les activitats científiques. Merton va definir
                  l'\emph{ethos} de la Ciència com el conjunt de valors i
                  normes que es consideren obligatòries pels científics.
                  Els imperatius morals ---normes--- deriven directament de
                  l'objectiu i del mètode de la Ciència i són tant
                  prescripcions morals com tècniques. Merton afirma que
                  l'\emph{ethos} de la ciència moderna inclou quatre
                  conjunts d'imperatius institucionals: universalisme,
                  <<comunisme>>, desinterès i escepticisme organitzat. En
                  aquesta recerca el plantejament de Merton té un pes molt
                  important perquè una de les hipòtesis principals és que
                  les normes formals que regeixen el model de producció del
                  programari lliure tenen les seves arrels en l'\emph{ethos}
                  de la Ciència}
}

@Article{         merton:1968,
  author        = {Robert K. Merton},
  title         = {{The Matthew Effect in Science}},
  journal       = {Science},
  year          = {1968},
  volume        = {159},
  pages         = {56--63},
  number        = {3810},
  annote        = {Estudi clàssic de sociologia de la ciència. Merton
                  planteja que l'efecte Mateu consisteix en què els
                  científics eminents són desproporcionadament reconeguts
                  per les seves contribucions científiques mentres que els
                  científics menys reputats tendeixen a ser
                  desproporcionadament poc reconeguts per les seves
                  contribucions científiques, encara que les contribucions
                  siguin comparables a les dels científics eminents. Merton
                  destaca que aquest biaix en a favor dels científics
                  eminents es produeix, sobretot, en casos de col·laboració
                  entre científics i en casos de descobriments independents
                  múltiples. En aquesta recerca el cito com a base teòrica
                  per al concepte de <<\emph{preferential attachment}>>
                  associat al creixement de xarxes complexes. [URL consultat
                  setembre 2008]},
  url           = {http://www.garfield.library.upenn.edu/merton/matthew1.pdf}
}

